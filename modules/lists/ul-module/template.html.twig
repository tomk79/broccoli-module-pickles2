<ul{% if _ENV.vars.class %} class="{{ class }}"{% endif %}{% if _ENV.mode == 'canvas' %} style="padding-top: 3px;"{% endif %}>
{% for row in loop %}
{{ loopitem_start('loop') }}
	<li{% if _ENV.mode == 'canvas' %} style="padding-top: 3px;"{% endif %}>{{ row.modules }}</li>
{{ loopitem_end() }}{% endfor %}{{ appender('loop') }}
</ul>
